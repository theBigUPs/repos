
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - testsite.com </title>
</head>
<body>
    <header>
        <div>a div inside a 
            header </div>
            @await RenderSectionAsync("header",required:false)
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div>a div inside a footer</div>
        @await RenderSectionAsync("footer",required:false)
    </footer>
 
</body>
</html>
